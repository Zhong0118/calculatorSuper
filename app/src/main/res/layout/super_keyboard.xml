<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="4"
    android:columnCount="5"
    android:orientation="horizontal"
    android:rowCount="6">
    <Button
        android:id="@+id/number1s"
        style="@style/btn_super_style"
        android:layout_row="4"
        android:layout_column="1"
        android:text="1" />

    <Button
        android:id="@+id/number2s"
        style="@style/btn_super_style"
        android:layout_row="4"
        android:layout_column="2"

        android:text="2" />

    <Button
        android:id="@+id/number3s"
        style="@style/btn_super_style"
        android:layout_row="4"
        android:layout_column="3"

        android:text="3" />

    <Button
        android:id="@+id/number4s"
        style="@style/btn_super_style"
        android:layout_row="3"
        android:layout_column="1"

        android:text="4" />

    <Button
        android:id="@+id/number5s"
        style="@style/btn_super_style"
        android:layout_row="3"
        android:layout_column="2"

        android:text="5" />

    <Button
        android:id="@+id/number6s"
        style="@style/btn_super_style"
        android:layout_row="3"
        android:layout_column="3"

        android:text="6" />

    <Button
        android:id="@+id/number7s"
        style="@style/btn_super_style"
        android:layout_row="2"
        android:layout_column="1"

        android:text="7" />

    <Button
        android:id="@+id/number8s"
        style="@style/btn_super_style"
        android:layout_row="2"
        android:layout_column="2"

        android:text="8" />

    <Button
        android:id="@+id/number9s"
        style="@style/btn_super_style"
        android:layout_row="2"
        android:layout_column="3"

        android:text="9" />

    <Button
        android:id="@+id/number0s"
        style="@style/btn_super_style"
        android:layout_row="5"
        android:layout_column="2"

        android:text="0" />

    <Button
        android:id="@+id/clears"
        style="@style/btn_super_style"
        android:layout_row="1"
        android:layout_column="1"

        android:text="C"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/backs"
        style="@style/btn_super_style"
        android:layout_row="1"
        android:layout_column="2"

        android:text="CE"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/adds"
        style="@style/btn_super_style"
        android:layout_row="4"
        android:layout_column="4"

        android:text="+"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/mins"
        style="@style/btn_super_style"
        android:layout_row="3"
        android:layout_column="4"

        android:text="-"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/muls"
        style="@style/btn_super_style"
        android:layout_row="2"
        android:layout_column="4"

        android:text="×"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/divs"
        style="@style/btn_super_style"
        android:layout_row="1"
        android:layout_column="4"

        android:text="÷"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/dots"
        style="@style/btn_super_style"
        android:layout_row="5"
        android:layout_column="3"

        android:text="." />

    <Button
        android:id="@+id/equals"
        style="@style/btn_super_style"
        android:layout_row="5"
        android:layout_column="4"

        android:text="="
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/symbols"
        style="@style/btn_super_style"
        android:layout_row="1"
        android:layout_column="3"
        android:text="+/−"
        android:textColor="@color/button_one" />

    <Button
        android:id="@+id/percents"
        style="@style/btn_super_style"
        android:layout_row="5"
        android:layout_column="1"

        android:text="%" />
    <Button
        android:id="@+id/reciprocal"
        style="@style/btn_super_style"
        android:layout_row="5"
        android:layout_column="0"
        android:text="1/x"/>
    <Button
        android:id="@+id/factorial"
        style="@style/btn_super_style"
        android:layout_row="4"
        android:layout_column="0"
        android:text="x!"/>
    <Button
        android:id="@+id/tan"
        style="@style/btn_super_style"
        android:layout_row="3"
        android:layout_column="0"
        android:text="tan"/>
    <Button
        android:id="@+id/cos"
        style="@style/btn_super_style"
        android:layout_row="2"
        android:layout_column="0"
        android:text="cos"/>
    <Button
        android:id="@+id/sin"
        style="@style/btn_super_style"
        android:layout_row="1"
        android:layout_column="0"
        android:text="sin"/>
    <Button
        android:id="@+id/lg"
        style="@style/btn_super_style"
        android:layout_row="0"
        android:layout_column="0"
        android:text="lg"/>
    <Button
        android:id="@+id/ln"
        style="@style/btn_super_style"
        android:layout_row="0"
        android:layout_column="1"
        android:text="ln"/>
    <Button
        android:id="@+id/sqrt"
        style="@style/btn_super_style"
        android:layout_row="0"
        android:layout_column="2"
        android:text="√"/>
    <Button
        android:id="@+id/left_bracket"
        style="@style/btn_super_style"
        android:layout_row="0"
        android:layout_column="3"
        android:text="("/>
    <Button
        android:id="@+id/right_bracket"
        style="@style/btn_super_style"
        android:layout_row="0"
        android:layout_column="4"
        android:text=")"/>

</GridLayout>